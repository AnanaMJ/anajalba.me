  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./build",
    "prod": "run-s clean build start",
    "dev": "run-s clean build-dev start-dev",
    "build": "run-p build-client build-server",
    "build-dev": "run-p build-client-dev build-server-dev",
    "build-client": "cross-env NODE_ENV=production webpack -p",
    "build-client-dev": "cross-env NODE_ENV=development webpack --progress",
    "build-server": "tsc",
    "build-server-dev": "tsc --diagnostics",
    "start": "cross-env NODE_ENV=production node ./build/server/app.js",
    "start-dev": "cross-env NODE_ENV=development node ./build/server/app.js",
    "// linters": "//",
    "lint": "run-s lint-src lint-style",
    "lint-fix": "run-s lint-src-fix lint-style-fix",
    "lint-src": "eslint --ext ts,tsx,js,jsx src",
    "lint-src-fix": "eslint --fix --ext ts,tsx,js,jsx src",
    "lint-style": "prettier -c \"src/**/*.{css,scss}\"",
    "lint-style-fix": "prettier --write \"src/**/*.{css,scss}\""
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/compression": "^0.0.36",
    "@types/express": "^4.16.0",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-router": "^5.0.0",
    "@types/react-router-dom": "^5.0.0",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.4",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^6.5.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser": "^3.11.0",
    "terser-webpack-plugin": "^1.1.0",
    "ts-loader": "^5.4.5",
    "typescript": "^4.0.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
